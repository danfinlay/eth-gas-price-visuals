{
  "name": "eth-gas-price-visuals",
  "version": "1.0.1",
  "description": "A boilerplate for react-hyperscript using browserify with beefy and ES6.",
  "main": "app/components/bar-chart.js",
  "scripts": {
    "start": "beefy index.js:bundle.js --live --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify index.js -o bundle.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      "brfs"
    ]
  },
  "author": "Dan Finlay",
  "license": "ISC",
  "dependencies": {
    "ethjs": "^0.2.4",
    "metamascara": "^1.1.1",
    "percentile": "^1.2.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-hyperscript": "^2.4.0",
    "react-redux": "^4.4.5",
    "recharts": "^1.0.0-beta.6",
    "redux": "^3.5.2",
    "redux-logger": "^2.3.1",
    "redux-thunk": "^1.0.2",
    "xtend": "^4.0.1"
  },
  "keywords": [
    "react",
    "hyperscript",
    "beefy",
    "browserify",
    "boilerplate"
  ],
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "beefy": "^2.1.8",
    "brfs": "^1.4.3",
    "browserify": "^14.0.0",
    "eth-block-tracker": "^2.2.2",
    "watchify": "^3.9.0"
  }
}
